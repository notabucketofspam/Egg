{
  "name": "webapp-v4",
  "version": "0.0.0",
  "description": "WebappV4",
  "main": "app.js",
  "author": {
    "name": "sbp"
  },
  "os": ["win32"],
  "cpu": ["x64"],
  "scripts": {
    "build": "tsc --build",
    "postbuild": "postbuild.bat",
    "clean": "tsc --build --clean && DEL final\\*.js",
    "push": "npm run build > nul && clasp push",
    "fix-gas-types": "replace-in-file \"declare var console\" \"//declare var _console\" node_modules/@types/google-apps-script/google-apps-script.base.d.ts",
    "update-dependencies": "ncu -u && npm i"
  },
  "devDependencies": {
    "@types/google-apps-script": "^1.0.37",
    "@types/node": "^16.4.1",
    "replace-in-file": "^6.2.0",
    "typescript": "^4.3.5"
  }
}
